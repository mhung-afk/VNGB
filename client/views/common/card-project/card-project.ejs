<div class="card-project">
    <div class="card-project-content">
        <h1 class="project-content-title"><%= title %> </h1>
        <div class="project-content-description"><%= description %> </div>
    </div>
    <div class="projects">
        <% products.forEach(product => { %>
            <div class="project-card">
                <a href="/product/<%= product.category.id %>/<%= product.id %>  ">
                    <div class="project-card-img"><img src="/<%= product.banner %> " alt=""></div>
                    <div class="project-card-content"><%= product.name %> </div>
                </a>
            </div>
        <% }) %>
    </div>    
</div>