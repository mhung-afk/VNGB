<div id="admin-products" class="mt-3">
    <% for(let i = start; i < (start + 4) ; i++) {%>
        <% if (projects[i]) { %>
            <div class="card mb-3" style="max-width: 540px;">
                <div class="row g-0">
                  <div class="col-md-4">
                    <img src="<%= projects[i].banner %>" class="img-fluid rounded-start">
                  </div>
                  <div class="col-md-8">
                    <div class="card-body">
                      <h5 class="card-title"><%= projects[i].name %> </h5>
                      <p class="card-text card-description"><%= projects[i].description %> </p>
                      <div class="btn-group">
                        <a href="/admin/product/edit?productID=<%= id %>&projectID=<%= projects[i].id %>" class="btn btn-primary">Edit</a>
                        <div data-path="/admin/product/<%= projects[i].id %>" data-redirect="/product?productID=<%= id %>" class="btn btn-danger delete-project">Delete</div>
                      </div>
                    </div>
                  </div>
                </div>
            </div>
        <% } %> 
    <% } %>
    <nav aria-label="Page navigation example">
        <ul class="pagination">
            <li class="page-item">
                <a class="page-link" href="/admin/product?productID=<%= id %>" aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
                </a>
            </li>
            <% for(var i = 0; i < projects.length / 4; i++) { %>
                    <li class="page-item"><a class="page-link" href="/admin/product?productID=<%= id %>&page=<%= i %>"><%= i + 1 %> </a></li>
            <% } %>
            <li class="page-item">
                <a class="page-link" href="/admin/product?productID=<%= id %>&page=<%= i - 1 %>" aria-label="Next">
                  <span aria-hidden="true">&raquo;</span>
                </a>
            </li>
        </ul>
    </nav>
</div>